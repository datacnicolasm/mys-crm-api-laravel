{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "jsx": "react-jsx",
        "moduleResolution": "bundler",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "resources/ts/*"
            ]
        },
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true
    },
    "include": [
        "resources/ts"
    ]
}